package org.job.java;
import java.util.Scanner;
import java.time.LocalDate;
class Date{
	int day;
	int month;
	int year;
	Date(){
		this(20,01,1994);
	}
	Date(int day,int month,int year){
		
		this.day=day;
		this.month=month;
		this.year=year;
	}
	void printRecord() {
		System.out.println(this.day+" / "+this.month+" / "+this.year);
	}
}
class Program{
	public static void main(String args[]) {
		Date dt1=new Date();
		dt1.printRecord();
		Date dt2=new Date(20,01,1997);
		dt2.printRecord();
	}
}