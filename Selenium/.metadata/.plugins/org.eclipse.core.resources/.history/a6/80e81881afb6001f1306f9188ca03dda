package org.job.java;
import java.util.Scanner;
import java.time.LocalDate;
class Date{
	int day;
	int month;
	int year;
	Date (){
		System.out.println("inside const");
		this.day=0;
		this.month=0;
		this.year=0;
	}
	Date (int day,int month,int year){
		System.out.println("inside const");
		this.day=day;
		this.month=month;
		this.year=year;
	}
}
class Program{
	public static void main(String args[]) {
		Date dt1=new Date(23,7,1994);
		Date dt2=new Date();
	}
}